---
import { getCollection } from "astro:content";

export const getSinglePage = async (collection: any) => {
  const allPage = await getCollection(collection);
  const removeIndex = allPage.filter((data: { id: string; }) => data.id.match(/^(?!-)/));
  const removeDrafts = removeIndex.filter((data: { data: { draft: any; }; }) => !data.data.draft);
  return removeDrafts;
};
